<configuration>

  <packageSources>
    <add key="github-hackney" value="https://nuget.pkg.github.com/LBHackney-IT/index.json" />
  </packageSources>

  <packageSourceCredentials>
    <github-hackney>
      <add key="Username" value="%HACKNEY_NUGET_USERNAME%" />
      <add key="ClearTextPassword" value= "%HACKNEY_PACKAGES_TOKEN%" />
    </github-hackney>
  </packageSourceCredentials>

  <activePackageSource>
    <add key="All" value="(Aggregate source)" />
  </activePackageSource>

</configuration>
